<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Login and Registration Suite">

    <listeners>
        <listener class-name="base.Listeners"/>
    </listeners>

    <!--<test thread-count="5" name="loginTest">-->
    <test name="login Positive Test">
        <classes>
            <class name="com.automationteststore.testCases.Login.LoginTestWithValidCredentials">
            </class>
        </classes>
    </test>

    <test name="login Negative Test">
        <classes>
            <class name="com.automationteststore.testCases.Login.LoginTestWithInvalidCredentials">
            </class>
        </classes>
    </test>

    <test name="RegistrationTestViaLoginLink">
        <classes>
            <class name="com.automationteststore.testCases.Registration.RegistrationTestViaLoginLink">
            </class>
        </classes>
    </test>

    <test name="RegistrationTestViaRegisterLink">
        <classes>
            <class name="com.automationteststore.testCases.Registration.RegistrationTestViaRegisterLink">
            </class>
        </classes>
    </test>

    <test name="Registration Test Using Faker Input">
        <classes>
            <class name="com.automationteststore.testCases.register.RegistrationTest_Using_Faker_Input">
            </class>
        </classes>
    </test>

    <test name="Registration Test Using Json Input">
        <classes>
            <class name="com.automationteststore.testCases.Registration.registrationTestUsingJsonInput">
            </class>
        </classes>
    </test>

    <test name="Registration Test Using Global Variables Input">
        <classes>
            <class name="com.automationteststore.testCases.Registration.RegistrationTest_Using_Global_Variables">
            </class>
        </classes>
    </test>

</suite>